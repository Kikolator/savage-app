{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint",
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ],
  "hosting": [
    {
      "target": "my",
      "public": "public/my",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "qr",
      "public": "public/qr",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "redirects": [
        {
          "source": "/",
          "destination": "https://savage-coworking.com/",
          "type": 301
        },
        {
          "source": "/2kHLrT",
          "destination": "https://savage-coworking.com/",
          "type": 302
        },
        {
          "source": "/4x7PQ2",
          "destination": "https://savage-coworking.com/es",
          "type": 302
        },
        {
          "source": "/Z4R9TQ",
          "destination": "https://savage-coworking.com/",
          "type": 302
        },
        {
          "source": "/H4K5ZQ",
          "destination": "https://instagram.com/savagecoworking",
          "type": 302
        },
        {
          "source": "/W3P7XQ",
          "destination": "https://instagram.com/savagecoworking",
          "type": 302
        },
        {
          "source": "/a9B3Zd",
          "destination": "https://instagram.com/savagecoworking",
          "type": 302
        },
        {
          "source": "/A7B2D9",
          "destination": "https://instagram.com/savagecoworking",
          "type": 302
        },
        {
          "source": "/B7aD1c",
          "destination": "https://instagram.com/savagecoworking",
          "type": 302
        },
        {
          "source": "/**",
          "destination": "https://savage-coworking.com/",
          "type": 301
        }
      ]
    }
  ],
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "hosting": {
      "port": 5010
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  }
}
